<%= form_for @band do |f| %>

<div>
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>

<div>
  <%= f.label :bio %>
  <%= f.text_area :bio %>
</div>

</div>
  <%= f.label :genre %>
  <%= f.text_area :genre %>
</div>

</div>
  <%= f.label :band_photo %>
  <%= f.file_field :band_photo %>
</div>

<p>
  <%= f.submit "Submit Band" %>
</p>

<% end %>
