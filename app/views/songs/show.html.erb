<h2><%= @song.title %></h2>
<p><%= @song.tempo %></p>

<%= link_to "Edit Song", edit_band_song_path(@band, @song) %>
<%= link_to "Delete Song", band_song_path(@band, @song), method: :delete %>

<br>

<%= link_to "Add New Track", new_song_track_path(@song) %>

<% @tracks.each do |track| %>
<br>
  <%= track.instrument %>
  <iframe width="70%" height="150" scrolling="no" frameborder="no" src="<%= track.soundcloud %>&single_active=false"></iframe>
<% end %>
